{
  "compilerOptions": {
      "outDir": "./dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "noUnusedLocals": true,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "preserveConstEnums": true,
      "removeComments": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "types": ["jest", "node"]
  },
  "include": [
      "./src/**/*"
  ]
}